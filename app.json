{
  "expo": {
    "name": "MedEscala",
    "slug": "medescala",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucaserib.medescala",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 3,
      "package": "com.lucaserib.medescala",
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "statusBarTranslucent": true,
      "navigationBarHidden": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "medescala",
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c236b910-f135-4d1f-81e6-04e69e64fe7c"
      },
      "EXPO_PUBLIC_API_URL": "https://www.medescalaapp.com.br"
    },
    "owner": "lucaserib",
    "runtimeVersion": "1.0.2",
    "updates": {
      "url": "https://u.expo.dev/c236b910-f135-4d1f-81e6-04e69e64fe7c"
    }
  }
}
