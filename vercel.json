{
  "version": 2,
  "buildCommand": "node build-backend.js",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/dist/**"
    }
  }
}
