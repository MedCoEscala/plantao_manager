{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_URL": "https://www.medescalaapp.com.br",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "pk_live_Y2xlcmsubWVkZXNjYWxhYXBwLmNvbS5iciQ",
        "NATIVEWIND_OUTPUT": "native",
        "EXPO_UNSTABLE_CORE_AUTOLINKING": "1"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "image": "latest"
      },
      "cache": {
        "disabled": false,
        "key": "nativewind-production-v1",
        "paths": [
          "node_modules",
          ".expo",
          "global.css"
        ]
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    }
  }
}
