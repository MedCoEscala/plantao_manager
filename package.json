{
  "name": "medescala",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "dev": "expo start --dev-client",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "build:dev": "eas build --profile development",
    "build:prod": "npm run verify:nativewind && eas build --profile production",
    "vercel-build": "cd backend && npm install && npx prisma generate && npm run build",
    "generate:css": "npx tailwindcss -i ./app/styles/global.css -o ./app/styles/global.css.native.css --platform=native",
    "verify:nativewind": "node scripts/verify-nativewind.js",
    "postinstall": "npm run generate:css"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.9.6",
    "@clerk/types": "^4.47.0",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "^2.11.0",
    "@shopify/flash-list": "1.7.6",
    "ajv": "^8.17.1",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "expo": "^53.0.0",
    "expo-auth-session": "~6.1.4",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.3",
    "expo-device": "~7.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-local-authentication": "~16.0.4",
    "expo-location": "~18.1.4",
    "expo-notifications": "~0.31.2",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-sqlite": "~15.2.9",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.15",
    "expo-web-browser": "~14.1.6",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-calendar-strip": "^2.2.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.20.1",
    "react-native-modal": "^14.0.0-rc.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-swiper": "^1.6.0",
    "react-native-web": "^0.20.0",
    "uuidv4": "^6.2.13",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "main": "expo-router/entry",
  "private": true
}
