{
  "name": "medescala",
  "version": "1.1.31",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "dev": "expo start --dev-client",
    "prod-test": "NODE_ENV=production EXPO_PUBLIC_API_URL=https://www.medescalaapp.com.br EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_live_Y2xlcmsubWVkZXNjYWxhYXBwLmNvbS5iciQ expo start --no-dev",
    "prod-test-clean": "expo start --clear --no-dev",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "build:dev": "eas build --profile development",
    "build:prod": "eas build --profile production --clear-cache",
    "build:debug": "DEBUG=nativewind eas build --profile production --clear-cache",
    "build:test": "eas build --profile production --clear-cache --non-interactive",
    "build:final": "eas build --platform android --profile production",
    "vercel-build": "cd backend && npm install && npx prisma generate && npm run build",
    "test-production": "node scripts/test-production-build.js",
    "quick-test": "node scripts/quick-playstore-test.js",
    "verify:nativewind": "node scripts/verify-nativewind.js",
    "test:path-aliases": "node scripts/test-path-aliases.js",
    "fix:nativewind": "node scripts/fix-nativewind-production.js",
    "upgrade:nativewind": "node scripts/upgrade-nativewind-v4.js",
    "fix:versions": "node scripts/fix-version-conflicts.js",
    "build:production": "npx expo export --platform android --output-dir ./dist-android",
    "test:production": "npx expo export --platform all --clear && npx expo serve",
    "build:android": "npx eas build --platform android --profile production",
    "build:android-local": "npx eas build --platform android --profile production --local",
    "build:apk-test": "npx eas build --platform android --profile production-test --local",
    "submit:android": "npx eas submit --platform android --profile production",
    "build:submit": "npx eas build --platform android --profile production --auto-submit",
    "eas:login": "npx eas login",
    "check:production": "node scripts/production-build-check.js",
    "diagnose:nativewind": "node scripts/test-nativewind-production.js"
